# Takes the base URL and query parameters to return json
returnJSON <- function(base, query) {
response <- GET(base, query)
if(response$status_code == 200) {
body <- content(response, "text")
json <- fromJSON(body)
return(json)
} else {
print("GET response is invalid")
return(NULL)
}
}
